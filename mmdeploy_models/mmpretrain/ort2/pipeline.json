{
    "pipeline": {
        "input": [
            "img"
        ],
        "output": [
            "post_output"
        ],
        "tasks": [
            {
                "type": "Task",
                "module": "Transform",
                "name": "Preprocess",
                "input": [
                    "img"
                ],
                "output": [
                    "prep_output"
                ],
                "transforms": [
                    {
                        "type": "LoadImageFromFile"
                    },
                    {
                        "type": "EfficientNetCenterCrop",
                        "crop_size": 384,
                        "crop_padding": 0
                    },
                    {
                        "type": "Normalize",
                        "to_rgb": true,
                        "mean": [
                            127.5,
                            127.5,
                            127.5
                        ],
                        "std": [
                            127.5,
                            127.5,
                            127.5
                        ]
                    },
                    {
                        "type": "ImageToTensor",
                        "keys": [
                            "img"
                        ]
                    },
                    {
                        "type": "Collect",
                        "meta_keys": [
                            "pad_shape",
                            "valid_ratio",
                            "img_shape",
                            "filename",
                            "scale_factor",
                            "flip_direction",
                            "ori_shape",
                            "flip",
                            "img_norm_cfg",
                            "ori_filename"
                        ],
                        "keys": [
                            "img"
                        ]
                    }
                ]
            },
            {
                "name": "efficientnetv2",
                "type": "Task",
                "module": "Net",
                "is_batched": false,
                "input": [
                    "prep_output"
                ],
                "output": [
                    "infer_output"
                ],
                "input_map": {
                    "img": "input"
                },
                "output_map": {}
            },
            {
                "type": "Task",
                "module": "mmcls",
                "name": "postprocess",
                "component": "LinearClsHead",
                "params": {
                    "num_classes": 1000,
                    "in_channels": 1280,
                    "loss": {
                        "type": "CrossEntropyLoss",
                        "loss_weight": 1.0
                    },
                    "topk": 5
                },
                "output": [
                    "post_output"
                ],
                "input": [
                    "infer_output"
                ]
            }
        ]
    }
}